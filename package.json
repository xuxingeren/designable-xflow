{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "build:github": "cross-env BUILD_ENV=github umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@antv/xflow": "^1.0.48",
    "@formily/antd": "^2.1.9",
    "@formily/core": "^2.1.9",
    "@formily/react": "^2.1.9",
    "lodash": "^4.17.21",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "^3.5.26"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/webpack-env": "^1.17.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.26",
    "cross-env": "^7.0.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
